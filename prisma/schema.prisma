generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "libsql://leading-silhouette-mateo-amateur.turso.io"
}

model register {
  id           Int    @id @default(autoincrement())
  username     String
  userlastname String
  userEdge     Int
  response1    Int
  response2    Int
  response3    Int
  response4    Int

  @@unique([username, userlastname])
}
